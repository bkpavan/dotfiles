" Set syntax on
syntax on

" Indent automatically depending on filetype
filetype indent on
set autoindent

" Case insensitive search
set ic

" Higlhight search
set hls

" Wrap text instead of being on one line
set lbr

" Change colorscheme from default to delek
colorscheme delek

"syntax enable
"se nu
"set tabstop=4
"set shiftwidth=4
"set expandtab
"filetype on
"filetype plugin on
"set nocp
"set hlsearch
"set incsearch

